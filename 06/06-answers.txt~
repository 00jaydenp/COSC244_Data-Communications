9. public class printWords{
          public static void main(String[] args){
                 Scanner sc = new Scanner(System.in);
                 String word = sc.next();
                 int num = sc.next();
                 for(int i = 0; i < num; i++){
                         System.out.print(word + " ");
                         }
                   }
}
                   
